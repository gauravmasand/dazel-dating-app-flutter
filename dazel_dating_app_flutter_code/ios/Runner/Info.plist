    <string>To take picture we need permission</string>
	<key>NSMicrophoneUsageDescription</key>
    <string>To send voice note we need microphone permission</string>
	<key>NSPhotoLibraryUsageDescription</key>
    <string>To upload images on profile and send media to chat we need permission</string>
	<key>NSContactsUsageDescription</key>
    <string>We'll now show your profile to your contacts</string>
	<key>NSLocationWhenInUseUsageDescription</key>
    <string>We'll show only nearby people to you</string>
	<key>PermissionGroupNotification</key>
    <string>You'll get notification of chats, new matches, likes and etc</string>
    <key>UIBackgroundModes</key>
    <array>
        <string>fetch</string>
        <string>remote-notification</string>
    </array>


